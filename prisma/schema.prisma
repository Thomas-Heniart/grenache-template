generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url = "file:./etc/dev.db"
}

model Order {
  id String @id
  side String
  status String
  symbol String
  price Int
  quantity Int
  makerId String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model Trade {
  id String @id
  symbol String
  price Int
  quantity Int
  buyerId String
  sellerId String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
